#!/bin/bash

#Skript m채채rab vanuse p천hjal, kes sa oled

#Sisesta vanus
echo -n "Sisesta (oma) vanus: "
read vanus

#Tingimused
if [ $vanus -ge 0 -a $vanus -lt 11 ]; then
	echo "Oled laps"
elif [ $vanus -ge 11 -a $vanus -lt 18 ]; then
	echo "Oled nooruk"
elif [ $vanus -ge 18 -a $vanus -lt 63 ]; then
	echo "Oled t채iskasvanu"
elif [ $vanus -ge 63 ]; then
	echo "Oled senjoor"
else
	echo "Vigane sisend"
fi
